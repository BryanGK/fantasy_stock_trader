<?xml version="1.0" encoding="UTF-8" ?>

<hibernate-mapping
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns="urn:nhibernate-mapping-2.2"
        namespace="API.Models"
        assembly="API">
    <class name="UserWalletModel" table="wallet">
        <id name="UserId" column="user_id" type="guid"><generator class="native" /></id>
        <property name="Cash" column="cash" type="decimal" />
    </class>
</hibernate-mapping>